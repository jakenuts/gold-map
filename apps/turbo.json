{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".tsbuildinfo"]
    },
    "@gold-map/core#build": {
      "outputs": ["dist/**", ".tsbuildinfo"]
    },
    "@gold-map/data-sources#build": {
      "dependsOn": ["@gold-map/core#build"],
      "outputs": ["dist/**", ".tsbuildinfo"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "cache": false
    }
  }
}
